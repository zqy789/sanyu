<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:focusableInTouchMode="true">

    <com.ybkj.syzs.deliver.ui.view.TimeSortView
        android:id="@+id/timeSortView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <com.ybkj.syzs.deliver.ui.view.refreshlayout.XRefreshLayout
        android:id="@+id/order_refresh"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/timeSortView">

        <com.ybkj.syzs.deliver.ui.view.recyclerview.XRecyclerView
            android:id="@+id/order_recycle"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
    </com.ybkj.syzs.deliver.ui.view.refreshlayout.XRefreshLayout>
</RelativeLayout>